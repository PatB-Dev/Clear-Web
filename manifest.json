{
    "manifest_version": 2,
    "name": "Clear web",
    "version": "1.0",
    "description": "Clear web",
    "icons": {
        "128": "icons/kwan.png"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "sketch/load.js"
            ]
        }
    ],
    "browser_action": {
        "default_icon": "icons/kwan.png",
        "default_popup": "sketch/popup.html",
        "default_title": "Clear web",
        "browser_style": true
    },
    "web_accessible_resources": [
        "sketch/popup.html"
    ],
    "permissions": [
        "alarms",
        "contextMenus",
        "storage",
        "unlimitedStorage",
        "tabs",
        "activeTab",
        "notifications",
        "webRequest",
        "webRequestBlocking",
        "<all_urls>"
    ]
}